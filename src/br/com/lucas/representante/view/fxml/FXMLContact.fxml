      <TextField fx:id="txtEmail" layoutX="64.0" layoutY="21.0" prefHeight="26.0" prefWidth="299.0" promptText="Campo Obrigatório">
         <tooltip><Tooltip text="E-mail" /></tooltip>
      </TextField>
      <TextField fx:id="txtCargo" layoutX="444.0" layoutY="21.0" prefHeight="26.0" prefWidth="202.0">
         <tooltip><Tooltip text="Cargo" /></tooltip>
      </TextField>
      <TextField fx:id="txtNome" layoutX="64.0" layoutY="65.0" prefHeight="26.0" prefWidth="299.0">
         <tooltip><Tooltip text="Nome do Contato" /></tooltip>
      </TextField>
      <TextField fx:id="txtTelefone" layoutX="445.0" layoutY="65.0" prefHeight="26.0" prefWidth="202.0" promptText="(xx) xxxx-xxxx">
         <tooltip><Tooltip text="Telefone" /></tooltip>
      </TextField>
      <TextField fx:id="txtRG" layoutX="64.0" layoutY="108.0" prefHeight="26.0" prefWidth="146.0" promptText="xx.xxx.xxx-x">
         <tooltip><Tooltip text="RG" /></tooltip>
      </TextField>
      <TextField fx:id="txtCPF" layoutX="261.0" layoutY="108.0" prefHeight="26.0" prefWidth="146.0" promptText="xxx.xxx.xxx-xx">
         <tooltip><Tooltip text="CPF" /></tooltip>
      </TextField>
      <DatePicker fx:id="dpAniversario" layoutX="507.0" layoutY="108.0" prefHeight="26.0" prefWidth="138.0" promptText="xx/xx/xxxx">
